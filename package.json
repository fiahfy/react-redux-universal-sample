{
  "name": "react-redux-universal-sample",
  "version": "0.0.0",
  "description": "",
  "author": {
    "name": "fiahfy",
    "email": "d.fiahfy@gmail.com",
    "url": "http://fiahfy.blogspot.com/"
  },
  "homepage": "http://fiahfy.blogspot.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/fiahfy/react-redux-universal-sample"
  },
  "engines": {
    "node": ">=4.2.2"
  },
  "main": "server.js",
  "scripts": {
    "install": "npm run build",
    "start": "nodemon -V server.js -- --optimize_for_size --max_old_space_size=512 --gc_interval=100",
    "start:dev": "npm run watch & npm run nodemon",
    "start:hot": "npm run hot:client & npm run hot:server",
    "nodemon": "nodemon -V -w bundle.js bundle.js",
    "hot:client": "webpack-dev-server --config webpack/client.hot.babel.js",
    "hot:server": "webpack -w --config webpack/server.hot.babel.js & node server.js",
    "watch": "npm run build -- -w",
    "watch:client": "npm run build:client -- -w",
    "watch:server": "npm run build:server -- -w",
    "build": "webpack --config webpack/all.babel.js",
    "build:client": "webpack --config webpack/client.babel.js",
    "build:server": "webpack --config webpack/server.babel.js",
    "lint": "eslint src"
  },
  "dependencies": {
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "0.23.1",
    "exports-loader": "0.6.3",
    "fbjs": "0.8.0-alpha.3",
    "file-loader": "0.8.5",
    "history": "2.0.1",
    "hoist-non-react-statics": "1.0.5",
    "html-loader": "0.4.3",
    "imports-loader": "0.6.5",
    "isomorphic-fetch": "2.2.1",
    "json-loader": "0.5.4",
    "koa": "2.0.0-alpha.3",
    "koa-convert": "1.2.0",
    "koa-router": "7.0.1",
    "koa-static": "2.0.0",
    "markdown-loader": "0.1.7",
    "node-loader": "0.5.0",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-helmet": "3.0.0",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-router-redux": "3.0.0",
    "redux": "3.3.1",
    "redux-async-connect": "1.0.0-rc4",
    "redux-devtools": "3.2.0",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.9",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "sass-loader": "3.2.0",
    "serialize-javascript": "1.2.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14"
  },
  "devDependencies": {
    "babel-eslint": "5.0.0",
    "eslint": "2.2.0",
    "eslint-plugin-babel": "3.1.0",
    "eslint-plugin-react": "4.2.0",
    "nodemon": "1.9.1",
    "webpack-dev-server": "1.14.0"
  }
}
