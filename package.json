  "engines": {
    "npm": ">=3.10.0",
    "node": ">=6.7.0"
  },
  "main": "bundle.js",
  "scripts": {
    "install": "npm run build",
    "start": "npm run watch & npm run nodemon",
    "start:hot": "npm run hot:client & npm run hot:server",
    "nodemon": "nodemon -V -w bundle.js bundle.js",
    "hot:client": "webpack-dev-server --config webpack.config.client.hot.babel.js",
    "hot:server": "webpack -w --config webpack.config.server.hot.babel.js & node bundle.js",
    "watch": "npm run build -- -w",
    "watch:client": "npm run build:client -- -w",
    "watch:server": "npm run build:server -- -w",
    "build": "webpack --config webpack.config.babel.js",
    "build:client": "webpack --config webpack.config.client.babel.js",
    "build:server": "webpack --config webpack.config.server.babel.js",
    "lint": "eslint src",
    "autofix": "eslint src --fix",
    "test": "npm run lint"
  },
  "dependencies": {
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "fbjs": "0.8.5",
    "isomorphic-fetch": "2.2.1",
    "koa": "2.0.0",
    "koa-bodyparser": "3.2.0",
    "koa-convert": "1.2.0",
    "koa-router": "7.0.1",
    "koa-static": "2.0.0",
    "material-ui": "0.17.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-router-scroll": "0.4.1",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-connect": "5.0.0",
    "redux-devtools": "3.3.2",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.2.0",
    "redux-logger": "2.8.1",
    "redux-thunk": "2.2.0",
    "serialize-javascript": "1.3.0",
    "webpack": "2.2.1"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "nodemon": "1.10.2",
    "webpack-dev-server": "2.3.0"
  }
}
