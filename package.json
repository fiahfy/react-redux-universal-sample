  "license": "MIT",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "bundle.js",
  "scripts": {
    "install": "npm run build",
    "start": "npm run watch & npm run nodemon",
    "start:hot": "npm run hot:client & npm run hot:server",
    "nodemon": "nodemon -V -w bundle.js bundle.js",
    "hot:client": "webpack-dev-server --config webpack.config.client.hot.babel.js",
    "hot:server": "webpack -w --config webpack.config.server.hot.babel.js & node bundle.js",
    "watch": "npm run build -- -w",
    "watch:client": "npm run build:client -- -w",
    "watch:server": "npm run build:server -- -w",
    "build": "webpack --config webpack.config.babel.js",
    "build:client": "webpack --config webpack.config.client.babel.js",
    "build:server": "webpack --config webpack.config.server.babel.js",
    "lint": "eslint src",
    "autofix": "eslint src --fix",
    "test": "npm run lint"
  },
  "dependencies": {
    "babel-loader": "6.2.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "isomorphic-fetch": "2.2.1",
    "koa": "2.0.0",
    "koa-bodyparser": "3.2.0",
    "koa-convert": "1.2.0",
    "koa-router": "7.0.1",
    "koa-static": "2.0.0",
    "material-ui": "0.15.4",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-helmet": "3.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.6.0",
    "redux-connect": "3.0.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.1.0",
    "scroll-behavior": "0.8.1",
    "serialize-javascript": "1.3.0",
    "webpack": "1.13.2"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-preset-react-hmre": "1.1.1",
    "eslint": "3.5.0",
    "eslint-config-airbnb": "11.1.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.2.2",
    "nodemon": "1.10.2",
    "webpack-dev-server": "1.15.1"
  }
}
