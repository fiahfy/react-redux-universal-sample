{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-use-before-define": 0,
    "semi": [2, "never"],
    "react/require-extension": 0,
    "react/prefer-stateless-function": 0,
    'generator-star-spacing': 0,
    'babel/generator-star-spacing': [2, { 'before': false, 'after': true }]
  }
}
